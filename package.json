{
  "name": "react-blog-service",
  "version": "1.0.0",
  "description": "react-blog-service",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "lint": "eslint .",
    "clean": "tsc -b --clean"
  },
  "dependencies": {
    "egg": "^3.15.0",
    "egg-mongoose": "^4.0.1",
    "egg-scripts": "^2.17.0"
  },
  "devDependencies": {
    "@eggjs/tsconfig": "^1.3.3",
    "@types/cheerio": "^0.22.32",
    "@types/mocha": "^10.0.1",
    "cheerio": "^1.0.0-rc.12",
    "egg-bin": "^6.5.2",
    "egg-mock": "^5.10.8",
    "eslint": "^8.50.0",
    "eslint-config-egg": "^13.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
